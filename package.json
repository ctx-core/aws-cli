{
	"name": "@ctx-core/aws-cli",
	"version": "12.0.3",
	"description": "ctx-core aws cli helpers",
	"keywords": [
		"ctx-core",
		"aws",
		"aws2",
		"cli"
	],
	"homepage": "https://github.com/ctx-core/aws-cli#readme",
	"bugs": {
		"url": "https://github.com/ctx-core/aws-cli/issues"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/ctx-core/aws-cli.git"
	},
	"license": "Apache-2.0",
	"author": "Brian Takita",
	"type": "module",
	"main": "./dist/index.js",
	"module": "./dist/index.js",
	"types": "./dist/index.d.ts",
	"dependencies": {
		"@ctx-core/child_process": "workspace:^7.0.2",
		"@ctx-core/cli-args": "workspace:^10.0.2",
		"@ctx-core/object": "workspace:^21.0.2"
	},
	"devDependencies": {
		"typescript": "^4.3.5"
	},
	"publishConfig": {
		"access": "public",
		"cache": "~/.npm"
	},
	"scripts": {
		"build": "npm run clean && npm run compile",
		"clean": "rm -f tsconfig.tsbuildinfo && rm -f src/**/*.js && rm -f src/**/*.d.ts && rm -f src/**/*.map",
		"compile": "./node_modules/.bin/tsc -b .",
		"exec": "$@"
	}
}
