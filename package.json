{
	"name": "@ctx-core/aws-cli",
	"version": "12.1.3",
	"description": "ctx-core aws cli helpers",
	"keywords": [
		"ctx-core",
		"aws",
		"aws2",
		"cli"
	],
	"homepage": "https://github.com/ctx-core/aws-cli#readme",
	"bugs": {
		"url": "https://github.com/ctx-core/aws-cli/issues"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/ctx-core/aws-cli.git"
	},
	"license": "Apache-2.0",
	"author": "Brian Takita",
	"type": "module",
	"types": "./lib/index.d.ts",
	"exports": {
		".": "./lib/index.js",
		"./package.json": "./package.json"
	},
	"scripts": {
		"build": ":",
		"clean": ":",
		"exec": "$@",
		"prepublishOnly": "npm run clean && npm run build && npm run test",
		"test": "pnpm test-unit && check-dts",
		"test-unit": "tsx node_modules/uvu/bin.js . '\\.test\\.(ts|js)$'",
		"test-unit-coverage": "c8 pnpm test-unit"
	},
	"dependencies": {
		"@ctx-core/child_process": "^7.2.2",
		"@ctx-core/cli-args": "^11.0.67",
		"@ctx-core/object": "^24.1.4"
	},
	"devDependencies": {
		"c8": "^7.12.0",
		"check-dts": "^0.7.0",
		"tsx": "^3.12.3",
		"typescript": "next",
		"uvu": "^0.5.6"
	},
	"publishConfig": {
		"access": "public",
		"cache": "~/.npm"
	},
	"svelte": "./lib/index.js",
	"sideEffects": false
}