{
	"name": "@ctx-core/aws-cli",
	"version": "11.0.0",
	"description": "ctx-core aws cli helpers",
	"keywords": [
		"ctx-core",
		"aws",
		"aws2",
		"cli"
	],
	"homepage": "https://github.com/ctx-core/aws-cli#readme",
	"bugs": {
		"url": "https://github.com/ctx-core/aws-cli/issues"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/ctx-core/aws-cli.git"
	},
	"license": "Apache-2.0",
	"author": "Brian Takita",
	"main": "./src/index.js",
	"module": "./src/index.js",
	"types": "./src/index.d.ts",
	"scripts": {
		"build": "npm run clean && npm run compile",
		"clean": "rm -f tsconfig.tsbuildinfo && rm -f dist/**/*.js && rm -f dist/**/*.d.ts && rm -f dist/**/*.map",
		"compile": "./node_modules/.bin/tsc -b .",
		"exec": "$@"
	},
	"dependencies": {
		"@ctx-core/child_process": "workspace:^6.0.0",
		"@ctx-core/cli-args": "workspace:^9.0.0",
		"@ctx-core/object": "workspace:^20.0.0"
	},
	"devDependencies": {
		"typescript": "^4.3.2"
	},
	"publishConfig": {
		"access": "public",
		"cache": "~/.npm"
	}
}
